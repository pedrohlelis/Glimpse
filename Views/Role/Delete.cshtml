@model Role
@{
    int? projectId = ViewData["projectId"] as int?;
}

<center>Deletar cargo? -> @Model.Name</center>
<div class="row">
    <input asp-for="Id" type="hidden" class="form-control"/>
        <div class="col-lg-12">
            <div class="card-footer">
                <center>
                    <button type="submit" class="btn-primary" asp-route-projectId="@projectId">Deletar</button>
                    <button class="btn-secondary"><a asp-action="MainProjects" asp-controller="Project" asp-route-projectId="@projectId">Cancelar</a></button>
                </center>
            </div>
        </div>
    </div>